{
   "author": "https://einc.io",
   "background": {
      "persistent": true,
      "scripts": [ "scripts/background.js" ]
   },
   "browser_action": {
      "default_icon": {
         "19": "images/icon-19.png",
         "38": "images/icon-38.png"
      },
      "default_popup": "popup.html",
      "default_title": "eIncMeta"
   },
   "commands": {
      "_execute_browser_action": {
         "suggested_key": {
            "chromeos": "Alt+Shift+M",
            "linux": "Alt+Shift+M",
            "mac": "Alt+Shift+M",
            "windows": "Alt+Shift+M"
         }
      }
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "scripts/contentscript.js" ],
      "matches": [ "file://*/*", "http://*/*", "https://*/*" ],
      "run_at": "document_start"
   } ],
   "default_locale": "en",
   "description": "eInc Wallet Browser Extension",
   "externally_connectable": {
      "matches": [ "https://eincmeta.einc.io/*" ]
   },
   "icons": {
      "128": "images/icon-128.png",
      "16": "images/icon-16.png"
   },
   "manifest_version": 2,
   "name": "eIncMeta",
   "permissions": [ "storage", "clipboardWrite", "http://localhost:8545/", "https://api.einc.io/" ],
   "short_name": "eIncMeta",
   "version": "3.14.9",
   "web_accessible_resources": [ "scripts/inpage.js" ]
}
